{
  "name": "react-figma",
  "version": "0.0.1-alpha.5",
  "description": "This plugin template uses Typescript. If you are familiar with Javascript, Typescript will look very familiar. In fact, valid Javascript code is already valid Typescript code.",
  "main": "./dist/index.js",
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "test": "jest",
    "prettify": "prettier \"src/**/*.{js,jsx,ts,tsx}\" \"examples/**/*.{js,jsx,ts,tsx}\" --ignore-path ./.prettierignore --write && git add . && git status",
    "build": "npm run build:clean && npm run build:lib && npm run build:js",
    "build:clean": "rimraf dist",
    "build:lib": "cross-env BABEL_ENV=production tsc -p tsconfig.json",
    "build:js": "copyfiles -u 1 src/**/*.js dist",
    "prepublish": "npm run build"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-babel"
  },
  "author": "Ilya Lesik <ilialesik@gmail.com>",
  "license": "MIT",
  "pre-commit": [
    "prettify",
    "tsc",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/react-figma/react-figma.git"
  },
  "keywords": [
    "figma",
    "react",
    "figma-plugins"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.18",
    "babel-jest": "^24.9.0",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "react-test-renderer": "16.8.6",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@types/yoga-layout": "^1.9.1",
    "nanoid": "^2.0.3",
    "object-assign": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-json-renderer": "^0.6.0",
    "rxjs": "^6.5.2",
    "scheduler": "^0.15.0",
    "yoga-layout": "^1.9.3"
  }
}
